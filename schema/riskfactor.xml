<?xml version="1.0"?>
<schema xmlns="http://www.w3.org/2000/10/XMLSchema">
  <element name="Risk factor">
    <complexType>
      <attribute name="isNegated" use="optional" >
          <simpleType>
              <restriction base="string">
                  <enumeration value="positive"/>
                  <enumeration value="negated"/>
              </restriction>
          </simpleType>
      </attribute>
    </complexType>
  </element>
</schema>
